DECLARE GRADE CHAR(1):='A';
BEGIN
CASE GRADE
WHEN 'A' THEN DBMS_OUTPUT.PUT_LINE('EXCELLENT');
WHEN 'B' THEN DBMS_OUTPUT.PUT_LINE('VEY GOOD ');
WHEN 'C' THEN DBMS_OUTPUT.PUT_LINE('WELL DONE');
WHEN 'D' THEN DBMS_OUTPUT.PUT_LINE('YOU PASSED');
WHEN 'E' THEN DBMS_OUTPUT.PUT_LINE('BETTER TRY AGAIN');
ELSE DBMS_OUTPUT.PUT_LINE('NO SUCH GRADE');
END CASE;
END;/


DECLARE GRADE CHAR(1):='A';
BEGIN
CASE
	WHEN GRADE = 'A' THEN DBMS_OUTPUT.PUT_LINE('EXCELLENT');
	WHEN GRADE = 'B' THEN DBMS_OUTPUT.PUT_LINE('VERY GOOD');
	WHEN GRADE = 'C' THEN DBMS_OUTPUT.PUT_LINE('WELL DONE');
	WHEN GRADE = 'D' THEN DBMS_OUTPUT.PUT_LINE('YOU PASSED');
	WHEN GRADE = 'E' THEN DBMS_OUTPUT.PUT_LINE('BETTER TRY AGAIN');
	ELSE DBMS_OUTPUT.PUT_LINE('NO SUCH STATEMENT');
END CASE;
END; /

DECLARE 
I NUMBER :=0;
BEGIN 
LOOP I := I+1;
EXIT WHEN I>10;
END LOOP;
DBMS_OUTPUT.PUT_LINE('VALUE OF I HAS REACHED (I)');
END; /
 



